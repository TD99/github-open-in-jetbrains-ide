{
  "manifest_version": 3,
  "name": "GitHub → JetBrains IDE",
  "version": "1.0",
  "description": "Adds 'Open with JetBrains IDE' to GitHub's clone menu.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "icons": {
    "16": "./assets/td99/icon16.png",
    "32": "./assets/td99/icon32.png",
    "48": "./assets/td99/icon48.png",
    "128": "./assets/td99/icon128.png"
  }
}
